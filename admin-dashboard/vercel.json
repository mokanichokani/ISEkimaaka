{
  "routes": [
    { "src": "/", "dest": "/admin.html" },
    { "src": "/admin", "dest": "/admin.html" },
    { "src": "/dashboard", "dest": "/admin.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
